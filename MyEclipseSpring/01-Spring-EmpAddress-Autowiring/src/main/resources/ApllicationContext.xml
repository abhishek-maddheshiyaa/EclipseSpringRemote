<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="empbean" class="com.cg.Employee" autowire = "byType">  
<property name="id">  <value>20</value>  </property>  
<property name="name">  <value>Alisa</value>  </property>  

</bean>  
 
<bean id="address1" class="com.cg.Address">  
<property name="City">  <value>Pune</value>  </property>  
<property name="State">  <value>Maharashtra</value>  </property>  
 </bean>
  
</beans>